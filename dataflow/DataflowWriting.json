{
	"name": "DataflowWriting",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"name": "parquetfiles"
				}
			],
			"sinks": [
				{
					"name": "sinkSQLPool",
					"rejectedDataLinkedService": {
						"referenceName": "synw-flights-bronze-layer-WorkspaceDefaultStorage",
						"type": "LinkedServiceReference"
					}
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> parquetfiles",
				"parquetfiles sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     store: 'synapse',",
				"     databaseType: 'spark',",
				"     format: 'table',",
				"     database: 'default',",
				"     tableName: 'flightsDB_historical',",
				"     recreate:true) ~> sinkSQLPool"
			]
		}
	}
}